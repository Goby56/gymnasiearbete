{
    "setup": {
        "structure": {
            "nodes": [784, 128, 128, 47],
            "activations": ["ReLU", "ReLU", "Softmax"]
        }
    },

    "training": {
        "accuracy_function": "Categorical",
        "loss_function": "CCE",
        "optimizer": {
            "function": "SGD",
            "args": {
                "decay": 0.001,
                "learn_rate": 0.3
            }
        },
        "dataset": "emnist-balanced.mat",
        "batch_size": 1024,
        "epochs": -1,
        "data_augmentation": {
            "noise": false,
            "shift": false,
            "rotate": false,
            "scale": false,
            "blur": false
        }
    }
}